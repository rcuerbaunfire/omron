<footer class="footer">
	<div class="container">
		<div class="footer__pre st-xl">
			<div class="footer__pre-inner">
				<div class="footer__pre-content">
					{% if prefooter_heading %}
						{% include 'components/heading.twig' with {
                            'class': 'footer__pre-heading text-white',
                            'value': prefooter_heading, 
                            'type': "h1",
                            'to_words': true
                        } %}
					{% endif %}

					{% include 'components/cta.twig' with {
                        'class' : 'filled blue',
                        'cta': prefooter_cta, 
                        'icon': true,
                    } %}
				</div>

				{% if prefooter_media_type == 'image' and prefooter_image %}
					<div class="footer__media">
						{% include 'components/image.twig' with {
                            'image': prefooter_image,
                            'size': [318, 382],
                        } %}
					</div>
				{% endif %}

				{% if prefooter_media_type == 'video' and prefooter_video %}
					<div class="footer__media !pointer-events-none">
						<div class="video-container loading" data-video-id="{{ prefooter_video }}">
							<div class="overlay"></div>
						</div>
					</div>
				{% endif %}
			</div>

			<div class="footer__to-top uline uline-light">
				<p class="text-white">{{ back_to_top }}</p>
				<svg class="h-[0.8125rem] w-[0.8125rem]" viewbox="0 0 13 13" fill="none" xmlns="http://www.w3.org/2000/svg">
					<g>
						<path d="M10.7871 4.53045L7.24536 1.13037L3.7036 4.53045" stroke="white" stroke-width="1.5" stroke-miterlimit="10"/>
						<path d="M0.588565 12.3229L6.11369 12.3229C6.73931 12.3229 7.24705 11.8151 7.24705 11.1895L7.24705 0.989258" stroke="white" stroke-width="1.5" stroke-miterlimit="10"/>
					</g>
				</svg>
			</div>
		</div>
	</div>

