				<div class="footer__form">
					{% include 'components/eyebrow.twig' with {
                        'text': footer_form_label,
                        'textClass': 'text-white-60',
                    } %}

					<div class="footer__form-inner">
						{% set region = footer_form.region %}
						{% set form_id = footer_form.form_id %}
						{% set portal_id = footer_form.portal_id %}
                        {% set lang_code = function("get_language") %}

                        {% if lang_code != "en" %}
                            {% set form_id = footer_form["form_id_" ~ lang_code] ?: footer_form.form_id %}
                        {% endif %}

						{% if region and form_id and portal_id %}
							<div class="form-container footer-form" data-region="{{ region }}" data-form-id="{{ form_id }}" data-portal-id="{{ portal_id }}"></div>
						{% endif %}
					</div>
				</div>

                <div class="footer__cookie-shortcut desktop">
                    <!-- OneTrust Cookies Settings button start -->
                    <button id="ot-sdk-btn" class="ot-sdk-show-settings">Cookie Settings</button>
                    <!-- OneTrust Cookies Settings button end -->
                </div>
			</div>

			{% if primary_footer_nav_columns|length %}
				<div class="footer__columns">
					{% for nav_column in primary_footer_nav_columns %}
						<div class="footer__column">
							{% for nav_item in nav_column.primary_footer_nav_item %}
								<div class="footer__group">
									{% if nav_item.child_nav_item|length %}
										{% for child in nav_item.child_nav_item %}
											{% if child.link %}
												{% set link_url = child.link.url ?: "#" %}
												{% set link_title = child.link.title %}
												{% set link_target = child.link.target ?: "_self" %}

												<a class="footer__item uline uline-light" href="{{ link_url }}" target="{{ link_target }}">
													<p>{{ link_title }}</p>
												</a>
											{% endif %}
										{% endfor %}
									{% endif %}
								</div>
							{% endfor %}

							{% if loop.last %}
								{% if footer_social|length %}
									<div class="footer__socials desktop">
										{% if footer_social_label %}
											<p class="text-white sm">{{ footer_social_label }}</p>
										{% endif %}

										<div class="footer__socials-inner">
											{% for social in footer_social %}
												{% set social_icon = social.icon %}
												{% set social_link = social.link ?: "#" %}

												<a href="{{ social_link|lower }}" target="_blank" class="footer__social" rel="nofollow noreferrer noopen">
													{% include 'components/image.twig' with {
                                                        'image': social_icon,
                                                        'class': 'object-contain',
                                                        'size': "full",
                                                    } %}
												</a>
											{% endfor %}
										</div>
									</div>
								{% endif %}
							{% endif %}
						</div>
					{% endfor %}
				</div>
			{% endif %}

			{% if footer_social|length %}
				<div class="footer__socials mobile">
					{% if footer_social_label %}
						<p class="text-white sm">{{ footer_social_label }}</p>
					{% endif %}

					<div class="footer__socials-inner">
						{% for social in footer_social %}
							{% set social_icon = social.icon %}
							{% set social_link = social.link ?: "#" %}

							<a href="{{ social_link|lower }}" target="_blank" class="footer__social" rel="nofollow noreferrer noopen">
								{% include 'components/image.twig' with {
                                    'image': social_icon,
                                    'class': 'object-contain',
                                    'size': "full",
                                } %}
							</a>
						{% endfor %}
					</div>
				</div>
			{% endif %}

            <div class="footer__cookie-shortcut mobile">
                <!-- OneTrust Cookies Settings button start -->
                <button id="ot-sdk-btn" class="ot-sdk-show-settings">Cookie Settings</button>
                <!-- OneTrust Cookies Settings button end -->
            </div>
		</div>
	</div>

	<div class="footer__foot">
		<div class="container">
			<div class="footer__foot-inner">
				{% if footer_credits %}
					<div class="footer__credits">{{ function('do_shortcode', footer_credits) }}</div>
				{% endif %}

				{% if secondary_footer_nav_item %}
					<div class="footer__foot-items">
						{% for nav_item in secondary_footer_nav_item %}
							{% if nav_item.link %}
								{% set link_url = nav_item.link.url ?: "#" %}
								{% set link_title = nav_item.link.title %}
								{% set link_target = nav_item.link.target ?: "_self" %}

								<a class="footer__item secondary uline uline-light" href="{{ link_url }}" target="{{ link_target }}">
									<p>{{ link_title }}</p>
								</a>
							{% endif %}
						{% endfor %}
					</div>
				{% endif %}
			</div>
		</div>
	</div>
</footer>
