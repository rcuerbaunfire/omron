																</div>

																<div class="nav__dd-main">
																	<div class="nav__dd-groups">
																		{% for group in nav_group %}
																			{% set nav_group_index = loop.index %}

																			<div class="nav__dd-group">
																				{% if group.is_title_a_link %}
																					{% if group.title_link %}
																						{% set link_url = group.title_link.url ?: "#" %}
																						{% set link_title = group.title_link.title %}
																						{% set link_target = group.title_link.target ?: "_self" %}

																						<a class="uline" href="{{ link_url }}" target="{{ link_target }}">
																							<p class="lg">{{ link_title }}</p>
																						</a>
																					{% endif %}
																				{% else %}
																					{% if group.title %}
																						<p class="lg">{{ group.title }}</p>
																					{% endif %}
																				{% endif %}

