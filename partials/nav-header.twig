																								{% set link_url = child.nav_link.url ?: "#" %}
																								{% set link_title = child.nav_link.title %}
																								{% set link_target = child.nav_link.target ?: "_self" %}

																								<a class="nav__dd-group-link" href="{{ link_url }}" target="{{ link_target }}" {{ is_products ? 'data-product-index="' ~ nav_index ~ nav_group_index ~ child_index ~ '"' }}>
																									<p>{{ link_title }}</p>
																								</a>
																							{% endif %}
																						{% endfor %}
																					</div>
																				{% endif %}
																			</div>
																		{% endfor %}
																	</div>

																	{% if is_products %}
																		<div class="nav__dd-images">
																			{% for group in nav_group %}
																				{% set nav_group_index = loop.index %}

																				{% if group.links|length %}
																					{% for child in group.links %}
																						{% set child_index = loop.index %}

																						{% if child.feature_image %}
																							<div class="nav__dd-image {{ nav_group_index == 1 and child_index == 1 ? " active" }}" data-product-index="{{ nav_index ~ nav_group_index ~ child_index }}">
                                                                                                <img class="lazy-image" data-src="{{ function("wp_get_attachment_image_src", child.feature_image, 'medium_large')[0] }}" alt="{{ function("get_the_title", child.feature_image) }}">
																							</div>
																						{% endif %}

																					{% endfor %}
																				{% endif %}
																			{% endfor %}
																		</div>
																	{% else %}
